###
# LIST WINNERS
GET {{ TENANT_BASE_URL }}/games/{{ gameId }}/winners?search=&gameId={{ gameId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json
X-Tenant-Subdomain: {{ subdomain }}
X-Tenant-ID: {{ tenantId }}


###
# CREATE WINNER
POST {{ TENANT_BASE_URL }}/games/{{ gameId }}/winners HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json
X-Tenant-Subdomain: {{ subdomain }}
X-Tenant-ID: {{ tenantId }}

{
  "templateId": "{{ cardTemplateId }}"
}


###
# RETRIEVE WINNER
GET {{ TENANT_BASE_URL }}/games/{{ gameId }}/winners/{{ winnerId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json
X-Tenant-Subdomain: {{ subdomain }}
X-Tenant-ID: {{ tenantId }}


###
# reject winner
POST {{ TENANT_BASE_URL }}/games/{{ gameId }}/winners/{{ winnerId }}/reject HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json
X-Tenant-Subdomain: {{ subdomain }}
X-Tenant-ID: {{ tenantId }}

{}


###
# verify winner
POST {{ TENANT_BASE_URL }}/games/{{ gameId }}/winners/{{ winnerId }}/verify HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json
X-Tenant-Subdomain: {{ subdomain }}
X-Tenant-ID: {{ tenantId }}


###
# sample data
OPTIONS {{ TENANT_BASE_URL }}/games/{{ gameId }}/winners?search= HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json
X-Tenant-Subdomain: {{ subdomain }}
X-Tenant-ID: {{ tenantId }}